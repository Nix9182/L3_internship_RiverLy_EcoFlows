#Creating a function to clean taxa names to use as a color filter
Clean_String <-function(string) {
new_string <- str_split(string, "_")[[1]][2]
new_string <- str_replace_all(new_string, "[^A-Z]", "")[[1]][1]
return(new_string)
}
